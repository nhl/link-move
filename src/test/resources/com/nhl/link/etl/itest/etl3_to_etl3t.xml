<?xml version="1.0" encoding="utf-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://nhl.github.io/link-etl/schema/extractor_config/extractor_config_1.xsd"
	xmlns="http://nhl.github.io/link-etl/schema/extractor_config">
	<type>jdbc</type>
	<connectorId>derbysrc</connectorId>
	<attributes>
		<attribute>
			<type>java.lang.Integer</type>
			<source>e2_id</source>
			<target>e2</target>
		</attribute>
		<attribute>
			<type>java.lang.Integer</type>
			<source>e5_id</source>
			<target>e5</target>
		</attribute>
		<attribute>
			<type>java.lang.String</type>
			<source>name</source>
			<target>name</target>
		</attribute>
		<attribute>
			<type>java.lang.String</type>
			<source>phone_number</source>
			<target>phoneNumber</target>
		</attribute>
	</attributes>
	<properties>
		<extractor.jdbc.sqltemplate>
			<![CDATA[SELECT e2_id, e5_id, name, phone_number FROM utest.etl3
			]]>
		</extractor.jdbc.sqltemplate>
		<extractor.jdbc.sqltemplate.caps>lower
		</extractor.jdbc.sqltemplate.caps>
	</properties>
</config>