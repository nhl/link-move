<?xml version="1.0" encoding="utf-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://nhl.github.io/link-etl/xsd/extractor_config_2.xsd"
	xmlns="http://nhl.github.io/link-etl/xsd/extractor_config_2.xsd">

	<type>atype</type>
	<connectorId>aconnector</connectorId>

	<extractor>

		<name>e1</name>
		<attributes>
			<attribute>
				<type>java.lang.String</type>
				<source>a1</source>
				<target>a_1</target>
			</attribute>
			<attribute>
				<type>java.lang.Integer</type>
				<source>a2</source>
				<target>a_2</target>
			</attribute>
			<attribute>
				<type>java.lang.Integer</type>
				<source>a2</source>
			</attribute>
		</attributes>

		<properties>
			<a.b>AB1</a.b>
			<x.y>XY1</x.y>
		</properties>
	</extractor>

	<extractor>

		<name>e2</name>
		<type>atype2</type>
		<connectorId>aconnector2</connectorId>

		<properties>
			<a.b>AB2</a.b>
			<x.y>XY2</x.y>
		</properties>
	</extractor>

	<extractor>
		<!-- default name -->
	</extractor>

</config>